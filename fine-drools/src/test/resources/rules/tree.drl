package rules;

import com.redbyte.wigen.core.domain.dto.Policy
import com.redbyte.wigen.core.domain.dto.Result;

global com.redbyte.wigen.core.domain.dto.Result res;

rule "rule A"
when
    Policy(sex == "男", age < 18, userSingle)
    then
    res.setResult("A", "rule A");
    end

rule "rule B"
when
    Policy(sex == "男", age < 18, !userSingle)
    then
    res.setResult("B", "rule B");
    end